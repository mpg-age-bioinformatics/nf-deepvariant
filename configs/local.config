process {
    withName:ucsc_to_ensembl {
        cpus=2
        container = "google/deepvariant:1.4.0"
        containerOptions = "-v ${params.project_folder}:/workdir"
    }
    withName:deepvariant {
        cpus=4
        container = "google/deepvariant:1.4.0"
        containerOptions = "-v ${params.project_folder}:/workdir -v ${params.deepvariant_raw_data}:/raw_data -v ${params.tmp}:/workdir/tmp"
    }
    withName:filtering {
        cpus=2
        container = "google/deepvariant:1.4.0"
        containerOptions = "-v ${params.project_folder}:/workdir -B ${params.deepvariant_raw_data}:/raw_data -B ${params.tmp}:/workdir/tmp"
    }
    withName:subtractWT {
        cpus=2
        container = "mpgagebioinformatics/rnaseq.python-3.8-2.sif"
        containerOptions = "-v ${params.project_folder}:/workdir -v ${params.samplestable}:/workdir/sample_sheet.xlsx"
    }
}